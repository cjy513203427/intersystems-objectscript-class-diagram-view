{
  "name": "intersystems-objectscript-class-diagram-view",
  "displayName": "InterSystems ObjectScript Class Diagram View",
  "description": "",
  "version": "0.2.0",
  "publisher": "JinyaoChen",
  "icon": "images/icon.png",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:objectscript",
    "onFileSystem:cls"
  ],
  "main": "./out/extension.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/cjy513203427/intersystems-objectscript-class-diagram-view.git"
  },
  "contributes": {
    "commands": [
      {
        "command": "intersystems-objectscript-class-diagram-view.generateClassDiagram",
        "title": "Generate Class Diagram"
      },
      {
        "command": "intersystems-objectscript-class-diagram-view.generateIntersystemsClassDiagram",
        "title": "Generate InterSystems Class Diagram"
      }
    ],
    "keybindings": [
      {
        "command": "intersystems-objectscript-class-diagram-view.generateClassDiagram",
        "key": "ctrl+alt+u",
        "when": "resourceExtname == .cls"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "intersystems-objectscript-class-diagram-view.generateClassDiagram",
          "when": "resourceExtname == .cls",
          "group": "z_commands"
        },
        {
          "command": "intersystems-objectscript-class-diagram-view.generateIntersystemsClassDiagram",
          "when": "editorLangId == objectscript",
          "group": "z_commands"
        }
      ],
      "explorer/context": [
        {
          "command": "intersystems-objectscript-class-diagram-view.generateClassDiagram",
          "when": "resourceExtname == .cls || explorerResourceIsFolder",
          "group": "z_commands"
        },
        {
          "command": "intersystems-objectscript-class-diagram-view.generateIntersystemsClassDiagram",
          "when": "workbenchState != empty",
          "group": "z_commands"
        }
      ],
      "commandPalette": [
        {
          "command": "intersystems-objectscript-class-diagram-view.generateIntersystemsClassDiagram",
          "when": "editorLangId == objectscript || workbenchState != empty"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.96.0",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.16.0",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "@types/axios": "^0.14.4",
    "axios": "^1.8.2"
  }
}
